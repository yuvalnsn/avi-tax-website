  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "cpx": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3"
  },
  "dependencies": {
    "alpinejs": "^3.13.10"
  }
}
