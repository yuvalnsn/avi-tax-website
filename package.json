{
  "name": "untitled20",
  "version": "1.0.0",
  "description": "",
  "main": "tailwind.config.js",
  "scripts": {
    "watch:tailwind": "npx tailwindcss -i ./src/css/input.css -o ./public/css/style.css --watch",
    "watch:files": "cpx \"src/**/*\" public --watch",
    "dev": "npm-run-all --parallel watch:*",
    "build": "npm-run-all build:*",
    "build:tailwind": "npx tailwindcss -i ./src/css/input.css -o ./public/css/style.css --minify",
    "build:assets": "cpx \"src/**/*\" public"
  },
  "keywords": [],
  "author": "",
